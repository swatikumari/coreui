<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
<Document>
    <name>KML file with polygon for bird species range</name>
    <description>Source various books and websites</description>
    <Style id="rangecolour">
        <LineStyle>
            <color>660000FF</color>
            <width>0.1</width>
        </LineStyle>
        <PolyStyle>
            <color>660000FF</color>
        </PolyStyle>
    </Style>
    <Style id="linecolour">
        <LineStyle>
            <color>660000FF</color>
            <width>3</width>
        </LineStyle>
    </Style>
    <Placemark>
        <name>Rungis</name>
        <description>Here is a KML example</description>
        <styleUrl>#rangecolour</styleUrl>
        <Polygon>
	    <description>A plolygon</description>
            <tessellate>1</tessellate>
            <altitudeMode>clampToGround</altitudeMode>
            <outerBoundaryIs>
                <LinearRing>
                    <coordinates>
                        12.9616883,77.6017112,0 
                        12.9619713276,77.6017867484,0
                        12.96175806,77.60176146,0
                        12.9614955398,77.6015829169,0
                        12.961915294,77.6017196542,0
                    </coordinates>
                </LinearRing>
            </outerBoundaryIs>
        </Polygon>
    </Placemark>
</Document>
</kml>
